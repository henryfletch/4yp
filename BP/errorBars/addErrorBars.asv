%I matrix: ((1)SNR, (2)MC simulations, (3BP iterations, block fails, err prob)
% Takes I and adds two additional columns 

function I = addErrorBars(I)

r = I(:,4); %Block errors per simulation
n = I(:,2); %Total blocks per simulation
I(:,5) = calcErrorBars('pos',r,n); %p+ value
I(:,6) = calcErrorBars('neg',r,n); %p- value
I(:,5) = I(:,5) - I(:,4); %P+ length is p+ - y
I(:,6) = I(:,4) - I(:,6); %P- length is y - p-

end