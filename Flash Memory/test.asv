%test script

clear
close

x = -5:0.0001:5;
N = 10000;
Vp = 2.6;
deltaVp = 0.25;
time = 3153600;

yProgrammed = mem_programState(x,Vp,deltaVp);
yRTN = mem_RTN(x,N);
yRetention = mem_retention(x,time,N);

output1 = conv(yProgrammed,yRTN,'same');
output2 = conv(output1,yRetention,'same');

area = trapz(x,output2);
output3 = output2/area;

plot(x,output3)
hold on
axis([0 5 0 10])

